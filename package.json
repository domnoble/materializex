{
  "name": "materializex",
  "version": "0.0.1",
  "author": "Dom Noble <dom@domnoble.com>",
  "description": "A Wordpress theme based on roots sage implementing materializecss, three.js, video.js and a few custom scripts",
  "homepage": "https://domnoble.com/",
  "repository": {
    "type": "git",
    "url": "git://github.com/domnoble/materializex.git"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "browserslist": [
    "last 2 versions",
    "android 4",
    "opera 12"
  ],
  "scripts": {
    "build": "webpack --progress --config resources/assets/build/webpack.config.js",
    "build:production": "webpack --progress -p --config resources/assets/build/webpack.config.js",
    "build:profile": "webpack --progress --profile --json --config resources/assets/build/webpack.config.js",
    "start": "webpack --hide-modules --watch --config resources/assets/build/webpack.config.js",
    "rmdist": "rimraf dist",
    "lint": "eslint resources/assets/scripts resources/assets/build",
    "test": "yarn run lint"
  },
  "engines": {
    "node": ">= 6.9.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "browser-sync": "^2.23.6",
    "browsersync-webpack-plugin": "^0.5.3",
    "bs-html-injector": "^3.0.3",
    "buble-loader": "^0.4.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-globs-webpack-plugin": "^0.2.0",
    "css-loader": "^0.28.9",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-webpack-plugin": "^1.6.1",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "postcss-loader": "^1.3.3",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.5.1",
    "url-loader": "^0.5.8",
    "video.js": "^6.7.3",
    "webpack": "^2.7.0",
    "webpack-assets-manifest": "^0.6.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0",
    "yargs": "^7.1.0"
  },
  "dependencies": {
    "audio-waveform": "^1.3.0",
    "draw-wave": "^2.0.0",
    "get-average-color": "0.0.6",
    "imagesloaded": "^4.1.4",
    "imports-loader": "^0.7.1",
    "jplayer": "^2.9.2",
    "jquery": "^3.3.1",
    "jquery-bridget": "^2.0.1",
    "masonry-layout": "^4.2.1",
    "materialize-css": "^0.98.2",
    "pickadate": "^3.5.6",
    "prismjs": "^1.10.0",
    "three": "^0.86.0",
    "whatwg-fetch": "^2.0.3"
  }
}
